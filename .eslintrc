{
  "parser": "babel-eslint",
  "globals": {},
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "strict": 0,
    "arrow-body-style": [0],
    "arrow-parens": [0],
    "camelcase": [0],
    "class-methods-use-this": [0],
    "comma-dangle": [0],
    "eqeqeq": [1],
    "flowtype-errors/show-errors": 2,
    "max-len": [0],
    "new-cap": [0],
    "no-console": [0],
    "no-param-reassign": [0],
    "no-underscore-dangle": [0],
    "no-unused-vars": [1],
    "prefer-arrow-callback": [1],
    "prefer-destructuring": [0],
    "quotes": [2, "double"],
    "spaced-comment": ["error", "always", { "markers": [":", "::"] }],
    "no-use-before-define": ["error", { "classes": false }]
  },
  "extends": [
    "airbnb-base",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "flowtype",
    "flowtype-errors"
  ],
}
